<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace，即命名空间，它的作用就是对SQL进行分类化管理 -->
<mapper namespace="com.dao.IManagerNewsDao">
    <insert id="insertManagerNews" >

        insert
        into manager_news (text,n_date)
        values
            (
             '${text}','${date}'
            )
    </insert>


    <select id="findManagerNewsAll"  resultType="ManagerNews">
        select *
        from manager_news

    </select>

    <delete id="deleteManagerNews" >
        delete  from manager_news where n_id = ${n_id}
    </delete>
</mapper>